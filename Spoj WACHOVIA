#include<bits/stdc++.h>
using namespace std;

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int n,w,t,i,j;
	cin>>t;
	while(t--){
		cin>>w>>n;
		int dp[n+1][w+1],val[n],we[n];
		for(i=0;i<n;i++)
		cin>>we[i]>>val[i];
		for(i=0;i<=n;i++){
			for(j=0;j<=w;j++){
				if(i==0 || j==0)
				dp[i][j]=0;
				else if(we[i-1]<=j)
				dp[i][j]=max(dp[i-1][j-we[i-1]]+val[i-1],dp[i-1][j]);
				else
				dp[i][j]=dp[i-1][j];
			}
		}
		cout<<"Hey stupid robber, you can get "<<dp[n][w]<<".\n";
	}
}
